  },
  "author": "zhao-jun",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zhao-jun/app-init-cli/issues"
  },
  "homepage": "https://github.com/zhao-jun/app-init-cli#readme",
  "devDependencies": {
    "cz-conventional-changelog": "^2.1.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
